{
	"compilerOptions": {
	  "outDir": "./js",
	  "noImplicitAny": false,
	  "module": "ES6",
	  "target": "ES2015",
	  "jsx":"preserve",
	  "allowJs": true,
	  "sourceMap": true,
	  "moduleResolution": "node",
	  "lib":["ES5", "DOM"]
	},
	"include": [
	  "src/**/*"
	],
	"exclude": [
		"node_modules",
		"**/*.spec.ts",
		"js/**/*"
	]
  }